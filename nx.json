{
  "npmScope": "npx-fire-ui",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "app-shell": {
      "tags": []
    },
    "example-app": {
      "tags": []
    },
    "example-app-e2e": {
      "tags": [],
      "implicitDependencies": ["example-app"]
    },
    "core": {
      "tags": []
    },
    "components": {
      "tags": []
    }
  },
  "workspaceLayout": {
    "appsDir": "projects",
    "libsDir": "modules"
  }
}
