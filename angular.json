{
  "version": 1,
  "projects": {
    "app-shell": {
      "projectType": "library",
      "root": "modules/app-shell",
      "sourceRoot": "modules/app-shell/src",
      "prefix": "npx-fire-ui",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "modules/app-shell/tsconfig.lib.json",
            "project": "modules/app-shell/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "modules/app-shell/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "modules/app-shell/tsconfig.lib.json",
              "modules/app-shell/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!modules/app-shell/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "modules/app-shell/jest.config.js",
            "tsConfig": "modules/app-shell/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "modules/app-shell/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": false
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest",
      "strict": true
    }
  },
  "defaultProject": "app-shell"
}
